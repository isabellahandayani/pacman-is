<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <template id="overview_siswa_tree" name="Siswa Tree">
        <t t-call="website.layout">
            <record model="ir.ui.view" id="overview_siswa_tree">
                <field name="name">siswa.tree</field>
                <field name="model">siswa</field>
                <field name="type">ir.actions.act_window</field>
                <field name='view_type'>tree,form</field>
                <field name='view_mode'>tree</field>
                <field name="arch" type="xml">
                    <tree string="data_siswa">
                        <field name="id_siswa" />
                        <field name="batch" />
                        <field name="nama" />
                        <field name="email" />
                        <field name="work" />
                        <field name="education" />
                        <field name="usia" />
                    </tree>
                </field>
            </record>
        </t>
    </template>
</odoo>